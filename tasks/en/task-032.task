DESCRIPTION {
    Look for areas with the magician's level lower 3.
}

PARSONS {
    SELECT 
    AREA 
    FROM WIZARDS
    WHERE 
    SKILL < 3 
    GROUP BY 
    AREA
    SKILL LOWER 3 #distractor
}

ANSWER {
   SELECT Area FROM WIZARDS WHERE SKILL < 3 GROUP BY AREA
}


TEST {
    TABLE {
        Wizards
        id|name|area|skill
        1|Anna|Shadowland|5
        2|Orz Orz|Kumpula|3
        3|Bingo|Shadowland|2
        4|Solmu|Shadowland|4
        5|Rufus|Kumpula|4
        6|Fred|Mumba|2
    }

    RESULT {
        Shadowland
        Mumba
    }
}

