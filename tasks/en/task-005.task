METADATA {
    id: task-005
    name: Tehtävä 5
    color: purple
}

DESCRIPTION {
    Hae kaikki 1700-luvulla syntyneet kummitukset.
}

PARSONS {
  SELECT name 
  FROM Test 
  WHERE YEAR > 2000
  WHERE YEAR > 1500 #distractor
  SELECT * #distractor
  WHERE YEAR > 1500 #distractor
}

TEST {
    TABLE {
        Ghosts
        id|name|year
        1|Arthur|1765
        2|Desiree|1801
        3|Siegfried|1650
        4|Sieglinde|1777
        5|Kaaleppi|1833
    }

    RESULT {
        Arthur
        Sieglinde
    }
}

